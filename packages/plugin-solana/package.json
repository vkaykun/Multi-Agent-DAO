{
    "name": "@elizaos/plugin-solana",
    "version": "0.25.6-alpha.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": null
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@anush008/tokenizers": "^0.2.0",
        "@coral-xyz/anchor": "0.28.0",
        "@elizaos/client-direct": "workspace:*",
        "@elizaos/client-discord": "workspace:*",
        "@elizaos/core": "workspace:*",
        "@elizaos/plugin-tee": "workspace:*",
        "@elizaos/plugin-trustdb": "workspace:*",
        "@solana/spl-token": "0.4.9",
        "@solana/web3.js": "1.95.8",
        "@types/pg": "8.11.10",
        "bignumber.js": "9.1.2",
        "bs58": "6.0.0",
        "fomo-sdk-solana": "1.3.2",
        "node-cache": "5.1.2",
        "pg": "8.13.1",
        "pumpdotfun-sdk": "1.3.2",
        "solana-agent-kit": "^1.4.0",
        "sqlite3": "^5.1.7",
        "vitest": "2.1.9"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.3",
        "ts-node": "^10.9.2",
        "tsup": "8.3.6"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "lint": "biome check src/",
        "lint:fix": "biome check --apply src/",
        "format": "biome format src/",
        "format:fix": "biome format --write src/",
        "test": "vitest run",
        "start:vela": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm src/startVela.ts",
        "start:kron": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm src/startKron.ts",
        "start:pion": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm src/startPion.ts",
        "re-embed-memories": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --esm scripts/reEmbedMemories.js"
    },
    "peerDependencies": {
        "form-data": "4.0.1",
        "whatwg-url": "7.1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
