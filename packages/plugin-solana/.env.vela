# .env.vela 

# Discord Configuration
DISCORD_API_TOKEN=
DISCORD_APPLICATION_ID=

# Database Configuration
DATABASE_TYPE=postgres
DATABASE_URL=postgres://localhost:
DATABASE_SSL=false
DATABASE_SCHEMA=public
DATABASE_POOL_SIZE=3
DATABASE_MAX_CONNECTIONS=5
DATABASE_IDLE_TIMEOUT=30000

# Process Configuration
MULTI_PROCESS=true
DEBUG=true
LOG_LEVEL=debug

# OpenAI Configuration
OPENAI_API_KEY=
USE_OPENAI_EMBEDDING=true
SMALL_OPENAI_MODEL=gpt-3.5-turbo
MEDIUM_OPENAI_MODEL=gpt-4
LARGE_OPENAI_MODEL=gpt-4
EMBEDDING_OPENAI_MODEL=text-embedding-ada-002
VECTOR_DIMENSION=1536

# Vela-specific Settings
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
PROPOSAL_QUORUM=10
PROPOSAL_MINIMUM_YES_VOTES=5
PROPOSAL_MINIMUM_VOTE_PERCENTAGE=51
MAX_PROPOSALS_PER_USER=3
PROPOSAL_EXPIRY_DAYS=7

# Solana Configuration
SOLANA_PRIVATE_KEY=
SOLANA_PUBLIC_KEY=
SOLANA_CLUSTER=mainnet-beta
SOLANA_RPC_URL=
SOL_ADDRESS=So11111111111111111111111111111111111111112
SLIPPAGE=1
BASE_MINT=So11111111111111111111111111111111111111112

# Embedding Controls - Standardize on 1536-dimensional vectors
DISABLE_EMBEDDINGS=false
USE_EMBEDDINGS=true
SKIP_EMBEDDING_GENERATION=false
ALLOW_DIMENSIONALITY_MISMATCH=true
VECTOR_DIMENSION_ADAPT=true
EMBEDDING_FALLBACK=true
EMBEDDING_MODEL=text-embedding-ada-002
